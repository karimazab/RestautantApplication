    <meta name="viewport" content="width=device-width">
    <script src="https://d3js.org/d3.v3.min.js" charset="utf-8"></script>
    <style>
        #barChart {
            font-size: 14px;
        }

        .bar {
            fill: steelblue;
        }

            .bar:hover {
                fill: brown;
            }

        .axis path,
        .axis line {
            fill: none;
            stroke: #000;
            shape-rendering: crispEdges;
        }

        .x.axis path {
            display: none;
        }
    </style>
